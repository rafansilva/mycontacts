{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": "off",
    "consistent-return": "off",
    "no-promise-executor-return": "off",
    "camelcase": "off",
    "no-unused-vars": ["error", {
      "argsIgnorePattern": "next"
    }]
  }
}
